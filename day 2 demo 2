


public class MyFstJavaCode {
	
	public static void main(String[] args) {

			int a = 200;
			byte b= (byte)a;
			
			
			double a1=50.5;
			float a2=(float)a1;
			
			System.out.println(a2);
	
			//by def compiler reads all decimal values as double
			
			float x=(float)10.5;
			float y=10.5f;
			
			//both are same
			
	}
	
	
}
// instance var , class var , local var 

