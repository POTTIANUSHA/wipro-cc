
public class MyFstJavaCode {
	
	int x=555; //instance 
	
	public void disp(){
		int x=50;
		System.out.println(x);
		System.out.println(this.x); //555
		//this keyword referes to object of current class
		//also used to resolve the ambiguity btwen local n instnc vars
	}
	
	public static void main(String[] args) {
		MyFstJavaCode m=new MyFstJavaCode();
		m.disp();// 50	
		System.out.println("main  "+m.x); //555
	}
}






