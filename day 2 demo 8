
public class MyFstJavaCode {
	long x;
	float y;
	
	Character z;
	Boolean test;
	MyFstJavaCode m;
	
	//all class types r null by def
	void disp(){
		System.out.println(x);
		System.out.println(y);
		System.out.println(z);// null
		System.out.println(test); //null 
		System.out.println(m); //null 
	}
	
		
	public static void main(String[] args) {

		new MyFstJavaCode().disp();	
		
	}
	
	
}


// instance var , class var , local var 

// 2
// premitive (8)  --- class type

//simple values

// 8 bits ,16 bits, 32 bits , 64 bits
// byte , short ,  int  ,   long
//	32 bit   64 bit
// float , double
// char -> 16bits
// boolean

//-------------------------class type

// class type- > objects including those which u create by urself 
// class -> user Def data type 
// Car , Dog , Calculator , String 


//type casting -. conversion of data type from one type to another

//-----------------------

/* 		wrapper classes -> to manipulate simple value (premitive) as objects 
 * byte    -- > Byte
 * short   --> Short
 * int   -> Integer
 * long   -> Long
 * float  -> Float
 * double  -> Double
 * char   ->Character
 * boolean -> Boolean
 */






