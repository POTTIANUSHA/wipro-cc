public class MyFstJavaCode {
	public static void main(String[] args) {
		int j=0;
		//2
		for(int i=0; i < 5 ;i++){
			
			for(j=0;j<2;j++)
			{
				System.out.println(" i "+ i+ " j "+j);
			}
			//j is always 2 when it comes here
			if(i==2 && j==1)
				break; // 5 , 6 
			// 10
		}
		

	}
}






